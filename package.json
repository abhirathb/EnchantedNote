{
  "name": "enchanted-notes",
  "version": "1.0.0",
  "description": "An Obsidian plugin where notes come alive and talk back to you",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "claude",
    "ai",
    "writing"
  ],
  "author": "Abhirath Batra",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/abhirathb/EnchantedNote.git"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.25.0",
    "obsidian": "^1.5.0",
    "tslib": "^2.6.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.52.0"
  }
}
